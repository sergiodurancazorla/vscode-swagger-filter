{
  "publisher": "sergioduran",
  "name": "swagger-filter",
  "displayName": "swagger filter",
  "description": "",
  "version": "0.0.1",
  "icon": "icon.png",
  "engines": {
    "vscode": "^1.80.0"
  },
  "categories": [
    "Other"
  ],
  "activationEvents": [],
  "main": "./out/extension.js",
  "contributes": {
    "commands": [
      {
        "command": "swagger-filter.previewFilteredSwagger",
        "title": "Preview Filtered Swagger"
      }
    ],
    "configuration": {
      "type": "object",
      "title": "Swagger Filter",
      "properties": {
        "swagger-filter.filterTag": {
          "type": "string",
          "default": "NOT EXPOSED",
          "description": "The tag to use for filtering the Swagger content."
        },
        "swagger-filter.filterParameter": {
          "type": "object",
          "default": {
            "name": "x-docs-read-only",
            "value": true
          },
          "description": "Parameter used to filter endpoints"
        }
      }
    }
  },
  "scripts": {
    "vscode:prepublish": "npm run compile",
    "compile": "tsc -p ./",
    "watch": "tsc -watch -p ./",
    "pretest": "npm run compile && npm run lint",
    "lint": "eslint src --ext ts",
    "test": "node ./out/test/runTest.js"
  },
  "devDependencies": {
    "@types/glob": "^8.1.0",
    "@types/js-yaml": "^4.0.5",
    "@types/mocha": "^10.0.1",
    "@types/node": "20.2.5",
    "@types/vscode": "^1.80.0",
    "@typescript-eslint/eslint-plugin": "^5.59.8",
    "@typescript-eslint/parser": "^5.59.8",
    "@vscode/test-electron": "^2.3.2",
    "eslint": "^8.41.0",
    "glob": "^8.1.0",
    "mocha": "^10.2.0",
    "swagger-ui-dist": "^5.1.1",
    "typescript": "^5.1.3"
  },
  "dependencies": {
    "js-yaml": "*",
    "yaml": "*"
  }
}